@{

}
<head>
    <script src="~/Includes/JS/CreateUser.js"></script>
    <link href="~/Includes/CSS/CreateUser.css" type="text/css" rel="stylesheet" />
</head>
<body>
    <header>
        <img src="~/Includes/Images/overzicht-homeknop.png" />
    </header>
    <main>
        <table>
            <tr>
                <td>Voornaam: </td>
                <td><input id="firstname" type="text" /></td>
            </tr>
            <tr>
                <td>Achternaam: </td>
                <td><input id="lastname" type="text" /></td>
            </tr>
            <tr>
                <td>E-mail: </td>
                <td><input id="email" type="email" /></td>
            </tr>
            <tr>
                <td>Wachtwoord: </td>
                <td><input id="password" type="password" /></td>
            </tr>
        </table>
        <table>
            <tr>
                <td>Gebruikerstype: </td>
                <td>
                    <select id="usertype" onchange="usertypechange()"><option value="0" selected>Leerling</option><option value="1">Docent</option><option value="2">Admin</option></select>
                </td>

            </tr>
            <tr>
                <td>
                    Geactiveerd:
                </td>
                <td>
                    <select id="activated"><option value="1" selected>Ja</option><option value="0">Nee</option></select>
                </td>
            </tr>
            <tr>
                <td>
                    StudieID:
                </td>
                <td>
                    <input type="number" id="studieid" />
                </td>
            </tr>
            <tr>
                <td><button onclick="createNew();">Aankmaken</button></td>
            </tr>
        </table>
    </main>
    <footer></footer>
</body>